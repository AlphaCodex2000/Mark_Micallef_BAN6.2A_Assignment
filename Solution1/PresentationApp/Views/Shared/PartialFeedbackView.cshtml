@if (ViewData["feedback"] != null)
{
    <div class="alert alert-primary" role="alert">
        @TempData["feedback"]
    </div>
}

@if (ViewData["warning"] != null)
{
    <div class="alert alert-danger" role="alert">
        @TempData["warning"]
    </div>
}